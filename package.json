{
  "name": "desafio-tecnico-laburen",
  "version": "1.0.0",
  "description": "WhatsApp AI Agent for Laburen - Customer Success Engineer Technical Challenge",
  "main": "src/index.ts",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "npx tsx src/index.ts",
    "dev": "npx tsx watch src/index.ts",
    "import-products": "npx tsx scripts/import-products.ts",
    "build": "chmod +x build.sh && ./build.sh",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "whatsapp",
    "ai",
    "agent",
    "gemini",
    "laburen",
    "typescript"
  ],
  "author": "",
